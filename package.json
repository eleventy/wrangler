{
  "name": "wrangler",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^5.0.0-alpha.37",
    "@material-ui/lab": "^5.0.0-alpha.37",
    "@material-ui/styles": "^5.0.0-alpha.35",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "diskspace": "^2.0.0",
    "drivelist": "^9.2.4",
    "electron-is-dev": "^2.0.0",
    "electron-settings": "^4.0.2",
    "filesize": "^6.3.0",
    "glob": "^7.1.7",
    "lodash": "^4.17.21",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "slash": "^4.0.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "start-electron": "nodemon --watch ./public/* --exec \"electron .\"",
    "start": "concurrently \"npm run start-react\" \"wait-on http://localhost:3000 && npm run start-electron\" ",
    "postinstall": "electron-builder install-app-deps",
    "pack-app": "npm run build-react && electron-builder --dir",
    "pack-build": "electron-builder  --dir",
    "build": "npm run build-react && electron-builder"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "electron": "^13.0.1",
    "electron-builder": "^22.10.5",
    "electron-rebuild": "^2.3.5",
    "nodemon": "^2.0.7",
    "wait-on": "^5.3.0"
  },
  "build": {
    "appId": "wrangler.eleventy.be",
    "productName": "Wrangler",
    "copyright": "Eleventy Media",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "build"
    }
  },
  "win": {
    "icon": "./build/images/favicon.ico",
    "target": [
      "7z"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
